{
  "framework": "jasmine",
  "launch_in_dev": ["PhantomJS"],
  "launch_in_ci": ["PhantomJS"],
  "before_tests": "coffee -cb spec/unit/*.spec.coffee",
  "on_exit": "rm spec/**/*.js",
  "src_files": [
    "app/app.js",
    "spec/**/*.spec.js"
  ],
  "serve_files": [
    "app/app.js",
    "spec/**/*.spec.js"
  ]
}